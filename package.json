{
  "name": "sakla",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev:pages": "wrangler pages dev .",
    "dev:web": "pnpm --filter oklinks-web dev",
    "build": "pnpm --filter ./apps/web build",
    "db:apply": "wrangler d1 execute sakla-db --file=db/schema.sql",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250224.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0",
    "wrangler": "^3.57.0"
  }
}
